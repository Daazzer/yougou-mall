.index {
  padding-bottom: 140rpx;
  padding-bottom: calc(140rpx + constant(safe-area-inset-bottom));
  padding-bottom: calc(140rpx + env(safe-area-inset-bottom));
}
.carousel {
  height: 340rpx;
}
.carousel__item image, 
.carousel__item navigator {
  width: 100%;
  height: 100%;
}
.cate-nav-list {
  display: flex;
  justify-content: space-around;
  padding-top: 12rpx;
  margin-bottom: 43rpx;
}
.cate-nav-list__item {
  width: 128rpx;
  height: 140rpx;
}
.cate-nav-list__item image,
.cate-nav-list__item navigator {
  width: 100%;
  height: 100%;
}
.recommend-list__item {
  margin-bottom: 40rpx;
}
.recommend-title {
  height: 60rpx;
  margin-bottom: 10rpx;
}
.recommend-list image,
.recommend-list navigator {
  width: 100%;
  height: 100%;
}
.recommend-pics {
  display: flex;
  padding: 0 12rpx;
  height: 386rpx;
}
.recommend-pics__main {
  width: 232rpx;
  margin-right: 15rpx;
}
.recommend-pics-list {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  width: 480rpx;
}
.recommend-pics-list__item {
  width: 233rpx;
  height: 188rpx;
  margin-bottom: 10rpx;
}
